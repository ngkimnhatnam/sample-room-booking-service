{
  "name": "sample-room-booking-service",
  "version": "1.0.0",
  "description": "A Node backend sample for room booking service",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p ./",
    "prestart": "npm run build",
    "start": "node ./dist/server.js",
    "predev": "npm run build",
    "dev": "nodemon --config nodemon.json src/server.ts -r dotenv/config ./dist/server.js dotenv_config_path=./dist/.env",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngkimnhatnam/sample-room-booking-service.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngkimnhatnam/sample-room-booking-service/issues"
  },
  "homepage": "https://github.com/ngkimnhatnam/sample-room-booking-service#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mysql": "^2.15.18",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1"
  }
}
